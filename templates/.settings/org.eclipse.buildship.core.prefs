# Eclipse Buildship Core Preferences
# This file contains generic buildship settings that should work across different environments
# Personal IDE settings have been removed to prevent environment-specific paths from being committed

# Gradle connection settings
connection.gradle.distribution=GRADLE_DISTRIBUTION(VERSION(8.9))
connection.project.dir=

# Build settings
auto.sync=true
build.scans.enabled=false
offline.mode=false
override.workspace.settings=true

# UI settings
show.console.view=true
show.executions.view=true

# Version
eclipse.preferences.version=1
